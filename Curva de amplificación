install.packages("pacman") #es un paquete que llama a otros paquetes y si no estan los instala
library("pacman") #esta función llama al paquete instalado

p_load("ggplot2","dplyr","vroom") #ggplot es para graficar, dplyr facilitar el manejo de datos y vroom llamar repositorios

Datos_curva_ <- vroom(file="https://raw.githubusercontent.com/ManuelLaraMVZ/resultados_PCR_practica/refs/heads/main/Amplif_grupo1_17022025.csv")
head(Datos_curva_) #solo para que me de los seis primeros datos

Curva_1 <- Datos_curva_ %>% #el nuevo objeto va a tener una asignación a partir de la base de datos XX, y le hacemos lo siguiente
  select("Cycle","A1")
Curva_1

Grafica_curva <- ggplot(Curva_1,
                        mapping = aes(x=Cycle,
                                      y=A1))+
  geom_point()+ 
  geom_line(color="red",size=1.2)

Grafica_curva

Grafica_curva_2 <- Grafica_curva+
  labs(x="ciclos",y="fluorescencia",
       title ="Creado por: Karla Gomez",
       subtitle="Grafica A1",
       caption="Ejercicio 3")+
  theme_classic()

Grafica_curva_2
